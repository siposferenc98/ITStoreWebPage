<a href = "@navLink" class="m-1 p-1 text-decoration-none d-flex flex-column justify-content-center align-items-center card categoryCard">
    <img class="card-img-top" style="height:100px; width:150px;" src="@picUrl"/>
    <div class="card-body">
        <h5 class="text-decoration-none text-white categoryCardText">@Text</h5>
    </div>
</a>


@code {
    [Parameter]
    public string Text { get; set; }
    [Parameter]
    public string picUrl { get; set; }
    [Parameter]
    public string navLink { get; set; }
}
